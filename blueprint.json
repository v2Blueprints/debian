{
  "identifier": "enginesd_debian_base",
  "about": {
    "title": "enginesd_debian_base",
    "explanation": "Docker Base for debian images"
  },
  "system_packages": {
    "adds": [
      "apt-transport-https",
      "unzip",
      "wget",
      "ca-certificates"
    ],
    "removes": [
      "bootlogd",
      "dmidecode",
      "gdbm-l10n",
      "iptables",
      "iputils-ping",
      "kmod",
      "krb5-locales",
      "less",
      "libiptc0",
      "libkmod2",
      "libnetfilter-conntrack3",
      "libnfnetlink0",
      "libnftnl11",
      "tasksel",
      "tasksel-data"
    ]
  },
  "bindings": [
    {
      "type": "embed",
      "target": {
        "repository": "https://github.com/v2Blueprints/locale_embed"
      }
    }
  ],
  "images": [
    {
      "runtimes": [
        "docker"
      ],
      "identifier": "debian",
      "output_identifier": "enginesd_debian_base"
    },
    {
      "runtimes": [
        "lxd"
      ],
      "identifier": "images:devuan/beowulf/cloud"
    }
  ]
}
