{
	"about": {
		"title": "enginesd_debian_base",
		"explanation": "Starting point of all docker debian images"
	},
	"binding_target": {
		"locale": "en_AU",
		"charset": "UTF-8"
	},
	"system_packages": {
		"adds": [
			"apt-transport-https",
			"locales-all",
			"unzip"
		],
		"removes": [
			"bootlogd",
			"dmidecode",
			"eudev",
			"gdbm-l10n",
			"iptables",
			"iputils-ping",
			"kmod",
			"krb5-locales",
			"less",
			"libip4tc0",
			"libip6tc0",
			"libiptc0",
			"libkmod2",
			"libnetfilter-conntrack3",
			"libnfnetlink0",
			"libnftnl11",
			"libx11-6",
			"libx11-data",
			"libxau6",
			"libxcb1",
			"libxdmcp6",
			"libxext6",
			"libxmuu1",
			"nano",
			"tasksel",
			"tasksel-data",
			"xauth"
		]
	},
	"packing": {
    "early": [
      "apt",
      "locale",
      "repos"
    ],		
		"last": [
      "cleanup"
    ]
  },
	"images": [
		{
			"type": "docker",
			"image": "debian"
		}
	]
}
